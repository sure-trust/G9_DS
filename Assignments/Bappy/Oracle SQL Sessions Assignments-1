1. select count(*) from emp;

2. select count(*) from dept;

3. select * from emp;

4. select * from dept;

5. select sum(sal)
   from emp;
   
6. select sum(comm)
   from emp;
   
7. select distinct job from emp
   where comm is not null;
   
8. select sysdate from dual; 

9. select avg(sal)
   from emp;
   
10. select deptno,count(*) "no of emp" 
    from emp 
    group by deptno;
    
 11. select deptno,sum(sal) "no of emp" 
     from emp 
     group by deptno;

 12. select job, count(*) 
      from emp
      group by job;
      
  13. select job, avg(sal) 
      from emp
      group by job;
      
  14. select to_char(hiredate,'DD')
      as hire_date,to_char(hiredate,'MM') 
      as hire_month,to_char(hiredate,'YYYY') 
      as hire_year from emp;
      
15.  select * 
     from emp 
     order by deptno;
     
16.   select * 
      from emp 
      order by job;  
      
17.   select * 
      from emp 
      order by sal desc;  
      
 18.   select * 
       from emp 
       order by deptno,sal desc;
       
 19.   select count(ename) 
      "name with 6 characters" from emp 
       where Ename like '______%';
    
 20.   select max(sal),min(sal) 
       from emp;
    
 21.   select deptno,max(sal) 
       "max",min(sal) 
       "min",avg(sal) 
       "avg",sum(sal) "sum" 
        from emp 
        group by deptno;
    
 22.    select * 
        from emp 
        order by hiredate;  
        
23.     select *
        from emp
        where hiredate = (select max(hiredate) 
        from emp);  
      
24.      select *
         from emp
         where hiredate = (select min(hiredate) 
         from emp);  
  
25.      select * 
         from emp 
         order by hiredate desc,deptno;         

26.      select empno,ename,sal
	     from emp
	     where sal >= (select (avg(sal))from emp);
 
 27.     select empno,ename,sal
	     from emp
         where sal <= (select (avg(sal))from emp);

28.      select empno , ename , sal
	     from emp 
	     where sal between 2000 and 4000;
